package stumng;

import javax.swing.*;
import java.awt.*;
import com.borland.jbcl.layout.*;

public class modifyinfFrame extends JFrame {
  XYLayout xYLayout1 = new XYLayout();
  JLabel jLabel1 = new JLabel();
  JTextField jTextField1 = new JTextField();
  JLabel jLabel2 = new JLabel();
  JTextField jTextField2 = new JTextField();
  JLabel jLabel3 = new JLabel();
  JTextField jTextField3 = new JTextField();
  JLabel jLabel4 = new JLabel();
  JTextField jTextField4 = new JTextField();
  JLabel jLabel5 = new JLabel();
  JTextField jTextField5 = new JTextField();
  JLabel jLabel6 = new JLabel();
  JTextField jTextField6 = new JTextField();
  JLabel jLabel7 = new JLabel();
  JTextField jTextField7 = new JTextField();
  JButton jButton1 = new JButton();

  public modifyinfFrame() {
    try {
      jbInit();
      this.setTitle("修改学生信息");
      this.setSize(500,300);
      this.setResizable(false);
      this.setLocation(200,300);
    }
    catch(Exception ex) {
      ex.printStackTrace();
    }
  }

  void jbInit() throws Exception {
    jLabel1.setFont(new java.awt.Font("Dialog", 0, 13));
    jLabel1.setText("姓名");
    this.getContentPane().setLayout(xYLayout1);
    jTextField1.setText("jTextField1");
    jLabel2.setFont(new java.awt.Font("Dialog", 0, 13));
    jLabel2.setText("性别");
    jTextField2.setText("jTextField2");
    jLabel3.setFont(new java.awt.Font("Dialog", 0, 13));
    jLabel3.setText("学号");
    jTextField3.setText("jTextField3");
    jLabel4.setFont(new java.awt.Font("Dialog", 0, 13));
    jLabel4.setText("民族");
    jTextField4.setText("jTextField4");
    jLabel5.setFont(new java.awt.Font("Dialog", 0, 13));
    jLabel5.setText("班级");
    jTextField5.setText("jTextField5");
    jLabel6.setFont(new java.awt.Font("Dialog", 0, 13));
    jLabel6.setText("出生日期");
    jTextField6.setText("jTextField6");
    jLabel7.setFont(new java.awt.Font("Dialog", 0, 13));
    jLabel7.setText("籍贯");
    jTextField7.setText("jTextField7");
    jButton1.setFont(new java.awt.Font("Dialog", 0, 13));
    jButton1.setText("修改");
    this.getContentPane().add(jLabel7,    new XYConstraints(95, 197, 51, 26));
    this.getContentPane().add(jButton1,  new XYConstraints(165, 243, 69, 30));
    this.getContentPane().add(jLabel1, new XYConstraints(38, 65, 43, 29));
    this.getContentPane().add(jLabel5, new XYConstraints(36, 149, 38, 32));
    this.getContentPane().add(jLabel3,  new XYConstraints(35, 100, 39, 38));
    this.getContentPane().add(jTextField3,    new XYConstraints(86, 111, 83, 28));
    this.getContentPane().add(jTextField5,     new XYConstraints(86, 156, 84, 27));
    this.getContentPane().add(jTextField1, new XYConstraints(85, 66, 85, 30));
    this.getContentPane().add(jLabel2, new XYConstraints(205, 64, 59, 30));
    this.getContentPane().add(jLabel4, new XYConstraints(205, 112, 45, 27));
    this.getContentPane().add(jLabel6, new XYConstraints(203, 157, 66, 23));
    this.getContentPane().add(jTextField2,  new XYConstraints(276, 65, 65, 29));
    this.getContentPane().add(jTextField4,  new XYConstraints(275, 114, 67, 25));
    this.getContentPane().add(jTextField6,  new XYConstraints(275, 155, 66, 25));
    this.getContentPane().add(jTextField7,   new XYConstraints(158, 198, 157, 25));
  }
}
